<%= render "layouts/errors", :object => @eat %>

<%= simple_form_for @eat do |f| %>
  <%= f.input :name %>
  <%= f.input :category %>
  <%= f.input :address %>
  <%= f.input :phone %>
  <%= f.input :website %>
  <%= f.input :hours %>
  <%= f.input :location %>
  <%= f.input :neighborhood %>
  <%= f.attachment_field :profile_image %>
  <%= f.label "Menu" %>
  <%= f.text_area :menu, class: 'form-control', rows: '10' %>
  <br>
  <%= f.button :submit %>
<% end %>
